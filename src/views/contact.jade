doctype html
html.no-js(lang='zxx')
  head
    // Mobile Specific Meta
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    // Favicon
    link(rel='icon', type='image/png', href='/images/icons/favicon.ico')
    // Author Meta
    meta(name='author', content='codepixer')
    // Meta Description
    meta(name='description', content='')
    // Meta Keyword
    meta(name='keywords', content='')
    // meta character set
    meta(charset='UTF-8')
    // Site Title
    title Change Your City
    link(href='https://fonts.googleapis.com/css?family=Poppins:100,200,400,300,500,600,700', rel='stylesheet')
    //
      CSS
      =============================================
    link(rel='stylesheet', href='stylesheets/home/linearicons.css')
    link(rel='stylesheet', href='stylesheets/home/font-awesome.min.css')
    link(rel='stylesheet', href='stylesheets/home/bootstrap.css')
    link(rel='stylesheet', href='stylesheets/home/magnific-popup.css')
    link(rel='stylesheet', href='stylesheets/home/nice-select.css')
    link(rel='stylesheet', href='stylesheets/home/hexagons.min.css')
    link(rel='stylesheet', href='stylesheets/home/animate.min.css')
    link(rel='stylesheet', href='stylesheets/home/owl.carousel.css')
    link(rel='stylesheet', href='stylesheets/home/main.css')
  body
    body
      header#header
        .container.main-menu
          .row.align-items-center.justify-content-between.d-flex
            #logo
              a(href='http://localhost:3000')
                img(src='images/Logo1.png', alt='', title='')
            nav#nav-menu-container
            ul.nav-menu
              li.menu-active
                a(href='http://localhost:3000') Home
              li
                a(href='http://localhost:3000/about') About
              li
                a(href='http://localhost:3000/proyectos') Proyectos
              li
                a(href='http://localhost:3000/desperfectos') Desperfectos
              li
                a(href='#') Contacto
              li
                if !isAuthenticated
                  a(href='http://localhost:3000/auth/signin') Conectar
                else
                  a(href="#")= user.firstName
              li
                if !isAuthenticated
                  a(href='http://localhost:3000/auth/signup') Registrar
              li
                if isAuthenticated
                  a(href="http://localhost:3000/auth/logout") Desconectar
            // #nav-menu-container
      // #header
      // start banner Area
      section#home.banner-area.relative
        .overlay.overlay-bg
        .container
          .row.d-flex.align-items-center.justify-content-center
            .about-content.col-lg-12
              h1.text-white
                | Contacto
              p.text-white.link-nav
                a(href='http://localhost:3000') Home 
                a -> 
                a(href='#')  Contacto
      // End banner Area
      // Start contact-page Area
      section.contact-page-area.section-gap
        .container
          .row
            #map.map-wrap(style='width:100%; height: 445px;')
            .col-lg-4.d-flex.flex-column.address-wrap
              .single-contact-address.d-flex.flex-row
                .icon
                  span.lnr.lnr-home
                .contact-details
                  h5 Málaga, Málaga
                  p 56/8, Teatinos
              .single-contact-address.d-flex.flex-row
                .icon
                  span.lnr.lnr-phone-handset
                .contact-details
                  h5 +34 666 69 69 69
                  p Lun a Vier 9am to 6 pm
              .single-contact-address.d-flex.flex-row
                .icon
                  span.lnr.lnr-envelope
                .contact-details
                  h5 ChangeYourCity@gmail.com
                  p ¡Envíamos tus dudas cuando quieras!
            .col-lg-8
              form#myForm.form-area(action='mail.php', method='post')
                .row
                  .col-lg-6.form-group
                    input.common-input.mb-20.form-control(name='name', placeholder='Introduzca su nombre', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter your name'", required='', type='text')
                    input.common-input.mb-20.form-control(name='email', placeholder='Introduzca su email', pattern='[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{1,63}$', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter email address'", required='', type='email')
                    input.common-input.mb-20.form-control(name='subject', placeholder='Introduzca el asunto', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter your subject'", required='', type='text')
                  .col-lg-6.form-group
                    textarea.common-textarea.form-control(name='message', placeholder='Mensaje', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Messege'", required='')
                  .col-lg-12.d-flex.justify-content-between
                    .alert-msg(style='text-align: left;')
                    button.genric-btn.primary.circle(style='float: right;') Enviar mensaje
      // End contact-page Area
      // Start brand Area
      section.brand-area.pt-120
        .container
          .row.align-items-center
            .col.single-brand
              img.img-fluid(src='images/Logos/logo_JANDA.png', alt='')
            .col.single-brand
              img.img-fluid(src='images/Logos/LogoUMA.jpg', alt='')
            .col.single-brand
              img.img-fluid(src='images/Logos/Logo_ETSII.jpg', alt='')
            .col.single-brand
              img.img-fluid(src='images/Logos/Logo_Link_Uma.png', alt='')
            .col.single-brand
              img.img-fluid(src='images/Logos/Logo_AYTO.png', alt='')
      // End brand Area
      // start footer Area
      footer.footer-area.section-gap
        .container
          .row
            .col-lg-3.col-md-12
              .single-footer-widget
                h6 Información adicional
                ul.footer-nav
                  li
                    a(href='#') Segmentant default page
                  li
                    a(href='#') Comentarios de los usuarios
                  li
                    a(href='#') Lista apps sociales del 2018
                  li
                    a(href='#') Artículo sobre los beneficios de su uso
            .col-lg-6.col-md-12
            .col-lg-3.col-md-12
              .single-footer-widget.mail-chimp
                h6.mb-20 Instragram Feed
                ul.instafeed.d-flex.flex-wrap
                  li
                    img(src='images/i1.jpg', alt='')
                  li
                    img(src='images/i2.jpg', alt='')
                  li
                    img(src='images/i3.jpg', alt='')
                  li
                    img(src='images/i4.jpg', alt='')
                  li
                    img(src='images/i5.jpg', alt='')
                  li
                    img(src='images/i6.jpg', alt='')
                  li
                    img(src='images/i7.jpg', alt='')
                  li
                    img(src='images/i8.jpg', alt='')
          .row.footer-bottom.d-flex.justify-content-between
            p.col-lg-8.col-sm-12.footer-text.m-0.text-white
              | Copyright ©
              script.
                document.write(new Date().getFullYear());
              |  Todos los derechos reservados
            .col-lg-4.col-sm-12.footer-social
              a(href='#')
                i.fa.fa-facebook
              a(href='#')
                i.fa.fa-twitter
              a(href='#')
                i.fa.fa-dribbble
              a(href='#')
                i.fa.fa-behance
      // end footer Area
